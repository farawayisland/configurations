{
  "chat.commandCenter.enabled": false,
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.fontWeight": "normal",
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": false,
  "editor.tabSize": 2,
  "json.schemas": [],
  "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font Mono', monospace",
  "terminal.integrated.fontLigatures": true,
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.fontWeightBold": "bold",
  "latex-workshop.latex.recipe.default" : "first",
  "latex-workshop.latex.recipes": [
  {
  "name": "lualatex",
  "tools": [
    "lualatex",
    "clean-temporary-files" // optional
  ]
  },
  {
  "name": "lualatex ➞ bibtex ➞ lualatex (2x)",
  "tools": [
    "lualatex",
    "bibtex",
    "lualatex",
    "lualatex"
  ]
  },
  {
  "name": "pdflatex",
  "tools": [
    "pdflatex",
    "clean-temporary-files"
  ]
  },
  {
  "name": "pdflatex ➞ bibtex ➞ pdflatex (2x)",
  "tools": [
    "pdflatex",
    "bibtex",
    "pdflatex",
    "pdflatex"
  ]
  },
  {
  "name": "xelatex",
  "tools": [
    "xelatex",
    "clean-temporary-files"
  ]
  },
  {
  "name": "xelatex ➞ bibtex ➞ xelatex (2x)",
  "tools": [
    "xelatex",
    "bibtex",
    "xelatex",
    "xelatex"
  ]
  }
  ],
  "latex-workshop.latex.tools": [
  {
    "name": "bibtex",
    "command": "bibtex",
    "args": [
    "%DOCFILE%"
    ],
    "env": {}
  },
  {
    "name": "clean-temporary-files",
    "command": "latexmk",
    "args": ["-c"]
  },
  {
  "name": "lualatex",
  "command": "latexmk",
  "args": [
    "-lualatex",
    "-file-line-error",
    "-interaction=nonstopmode",
    "-synctex=1",
    "-outdir=%OUTDIR%",
    "%DOC_EXT%"
  ],
  "env": {}
  },
  {
  "name": "pdflatex",
  "command": "latexmk",
  "args": [
    "-pdflatex",
    "-file-line-error",
    "-interaction=nonstopmode",
    "-synctex=1",
    "-outdir=%DIR%",
    "%DOC_EXT%"
  ],
  "env": {}
  },
  {
  "name": "xelatex",
  "command": "latexmk",
  "args": [
    "-xelatex",
    "-file-line-error",
    "-interaction=nonstopmode",
    "-synctex=1",
    "-outdir=%DIR%",
    "%DOC_EXT%"
  ],
  "env": {}
  },
  ],
  "latex-workshop.view.pdf.external.viewer.command": "sioyek",
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.applyToAllProfiles": [],
}
